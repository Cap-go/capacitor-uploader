{
  "name": "@capgo/capacitor-uploader",
  "version": "0.0.37",
  "description": "Upload file natively",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Sources",
    "ios/Tests",
    "Package.swift",
    "CapgoCapacitorUploader.podspec"
  ],
  "author": "Martin Donadieu <martin@capgo.app>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cap-go/capacitor-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/Cap-go/capacitor-uploader/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "uploader",
    "s3",
    "upload",
    "file",
    "upload",
    "upload-file",
    "upload-files",
    "upload-file-to-s3",
    "upload-files-to-s3",
    "capgo",
    "capacitor-uploader",
    "background",
    "background-upload",
    "background-upload-file",
    "background-upload-files",
    "background-upload-file-to-s3",
    "background-upload-files-to-s3"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "xcodebuild -scheme CapgoCapacitorUploader -destination generic/platform=iOS",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --autocorrect --format",
    "eslint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc.js \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api UploaderPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^6.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "prettier-plugin-java": "^2.6.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "swiftlint": "^1.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^6.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended",
    "rules": {
      "@typescript-eslint/prefer-optional-chain": "off"
    }
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
